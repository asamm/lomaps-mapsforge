#from actions.composite_op import lighten, darken, opacity
<?xml version="1.0" encoding="utf-8"?>
<rendertheme xsi:schemaLocation="http://mapsforge.org/renderTheme https://raw.githubusercontent.com/mapsforge/mapsforge/dev/resources/renderTheme.xsd" version="5"
			 base-stroke-width="0.7" map-background="#ebeade" xmlns="http://mapsforge.org/renderTheme" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<!--
		LOCUS INTERNAL THEME FOR LOMAPS
-->
	<stylemenu id="r4menu" defaultvalue="hike_bike" defaultlang="en">
		<!-- LAYER CITY -->
		<layer id="pub_rest_city" enabled="true" visible="false">
			<name lang="en" value="food_drink"/>
			<cat id="pub_rest_city"/>
		</layer>
		<layer id="accommodation_city" enabled="true" visible="false">
			<name lang="en" value="accommodation"/>
			<cat id="accommodation_city"/>
		</layer>
		<!-- LAYER SUMMER -->
		<layer id="cycling_routes" enabled="true" visible="false">
			<name lang="en" value="cycling_routes"/>
			<cat id="cyc_routes"/>
		</layer>
		<layer id="mtb_trails" enabled="true" visible="false">
			<name lang="en" value="mtb_trails"/>
			<cat id="cyc_mtb_routes"/>
		</layer>
		<layer id="hiking_trails" enabled="true" visible="false">
			<name lang="en" value="hiking_trails"/>
			<cat id="hik_routes"/>
		</layer>
		<layer id="via_ferrata" enabled="true" visible="false">
			<name lang="en" value="via_ferrata"/>
			<cat id="via_ferrata_cat"/>
		</layer>
		<layer id="pub_rest_topo" enabled="true" visible="false">
			<name lang="en" value="food_drink"/>
			<cat id="pub_rest_topo"/>
		</layer>
		<layer id="pistes_nordic" enabled="true" visible="false">
			<name lang="en" value="nordic_ski_trails"/>
			<cat id="pistes_nordic"/>
		</layer>
		<layer id="snowmobiles" enabled="true" visible="false">
			<name lang="en" value="snowmobile_trails"/>
			<cat id="snowmobiles"/>
		</layer>
		<!-- LAYER CAR -->
		<layer id="pub_rest_car" enabled="true" visible="false">
			<name lang="en" value="food_drink"/>
			<cat id="pub_rest_car"/>
		</layer>
		<!-- CAR -->
		<layer id="car" visible="true">
			<name lang="en" value="Car"/>
			<cat id="base_car"/>
			<cat id="accommodation_car"/>
			<cat id="highways_car"/>
			<cat id="shopping"/>
			<cat id="transportation_car"/>
			<overlay id="pub_rest_car"/>
		</layer>
		<!-- CITY -->
		<layer id="city" visible="true">
			<name lang="en" value="City"/>
			<cat id="no_car"/>
			<cat id="city"/>
			<cat id="base_city"/>
			<cat id="health_city"/>
			<cat id="highways_city"/>
			<cat id="religion_city"/>
			<cat id="shopping"/>
			<cat id="tourism_city"/>
			<cat id="transportation_city"/>
			<overlay id="pub_rest_city"/>
			<overlay id="accommodation_city"/>
		</layer>
		<!-- SUMMER -->
		<layer id="hike_bike" visible="true">
			<name lang="en" value="Hike, Bike"/>
			<cat id="no_car"/>
			<cat id="base_summer"/>
			<cat id="base_topo"/>
			<cat id="accommodation_topo"/>
			<cat id="natural_topo"/>
			<cat id="highways_topo"/>
			<cat id="health_topo"/>
			<cat id="shopping"/>
			<cat id="tourism_topo"/>
			<cat id="transportation_topo"/>
			<overlay id="cycling_routes"/>
			<overlay id="mtb_trails"/>
			<overlay id="hiking_trails"/>
			<overlay id="via_ferrata"/>
			<overlay id="pub_rest_topo"/>
		</layer>
		<!-- WINTER -->
		<layer id="ski" visible="true">
			<name lang="en" value="Ski"/>
			<cat id="no_car"/>
			<cat id="base_winter"/>
			<cat id="base_topo"/>
			<cat id="accommodation_topo"/>
			<cat id="natural_topo"/>
			<cat id="highways_topo"/>
			<cat id="pistes"/>
			<cat id="health_topo"/>
			<cat id="shopping"/>
			<cat id="snowmobiles_cat"/>
			<cat id="tourism_topo"/>
			<cat id="transportation_topo"/>
			<overlay id="pub_rest_topo"/>
			<overlay id="pistes_nordic"/>
			<overlay id="snowmobiles"/>
		</layer>
	</stylemenu>
	<!-- Render sea -->
	<rule e="way" k="*" v="*">
		<rule e="way" k="natural" v="sea">
			<area fill="#9CC9E8"/>
		</rule>
		<rule cat="base_car" e="way" k="natural" v="nosea">
			<area fill="#ebeade"/>
		</rule>
		<rule cat="base_city" e="way" k="natural" v="nosea">
			<area fill="#ebeade"/>
		</rule>
		<rule cat="base_summer" e="way" k="natural" v="nosea">
			<area fill="#ebeade"/>
		</rule>
		<rule cat="base_winter" e="way" k="natural" v="nosea">
			<area fill="#f7fbfe"/>
		</rule>
	</rule>
	<!-- =====================================================
						AREALS AND LINES
	====================================================== -->
	<rule e="any" k="*" v="*">
		<!-- Industrial and city areals-->
		<rule e="way" k="landuse|lm_landuse" v="*" closed="yes">
			<rule e="way" k="*" v="*" zoom-min="10" zoom-max="11">
				<rule e="way" k="lm_landuse" v="residential_city">
					<area fill="#c9c9c9"/>
				</rule>
			</rule>
			<rule e="way" k="*" v="*" zoom-min="12" zoom-max="12">
				<rule e="way" k="landuse" v="residential|retail|industrial|brownfield|railway|commercial|construction|greenfield|garages">
					<area fill="#D1D1D1"/>
				</rule>
				<rule e="way" k="lm_landuse" v="residential_city|residential_village">
					<area fill="#D1D1D1"/>
				</rule>
			</rule>
			<!-- SUMMER industrial, residential -->
			<rule cat="base_summer" e="way" k="*" v="*" zoom-min="13">
				<rule e="way" k="landuse|lm_landuse" v="residential_city|residential_village|farmyard|residential|commercial|garages|construction|greenfield|brownfield" zoom-max="14">
					<area fill="#e0d4b3"/>
				</rule>
				<rule e="way" k="landuse|lm_landuse" v="residential_city|residential_village|farmyard|residential|commercial|garages|construction|greenfield|brownfield" zoom-min="15">
					<area fill="#90e0d4b3"/>
				</rule>
				<rule e="way" k="landuse" v="industrial|brownfield|railway">
					<area fill="#90bfbfcf"/>
				</rule>
				<rule e="way" k="landuse" v="quarry">
					<area fill="#D6D6E1"/>
					<rule e="way" k="*" v="*" zoom-min="14">
						<line stroke="#797979" dy="0.7" stroke-width="1.5" stroke-dasharray="0.5,3" stroke-linecap="butt"/>
					</rule>
				</rule>
			</rule>
			<!-- CAR residential industrial -->
			<rule cat="base_car" e="way" k="*" v="*" zoom-min="13">
				<rule e="way" k="landuse|lm_landuse" v="residential_city|residential_village|residential|brownfield|industrial|brownfield|garages|construction|quarry" zoom-min="13" zoom-max="15">
					<area fill="#dcdcdc"/>
				</rule>
			</rule>
			<!-- City industrial -->
			<rule cat="base_city" e="way" k="*" v="*" zoom-min="13">
				<rule e="way" k="landuse|lm_landuse" v="residential_city|residential_village" zoom-max="15">
					<area fill="#dcdcdc"/>
				</rule>
				<rule e="way" k="landuse|lm_landuse" v="residential_city|residential_village" zoom-min="16">
					<area fill="#80dcdcdc"/>
				</rule>
				<rule e="way" k="landuse" v="brownfield|industrial|brownfield|garages|construction|quarry">
					<area fill="#90bfbfcf"/>
				</rule>
			</rule>
			<!-- winter industrial -->
			<rule cat="base_winter" e="way" k="*" v="*" zoom-min="13">
				<rule e="way" k="landuse|lm_landuse" v="residential_city|residential_village|residential|commercial|garages">
					<area fill="#E2E2E2"/>
				</rule>
				<rule e="way" k="landuse" v="quarry|farmyard|industrial|brownfield|railway|construction|greenfield|brownfield">
					<area fill="#CECECE"/>
				</rule>
			</rule>
		</rule>

		<!-- forest -->
		<rule e="way" k="natural|landuse" v="forest|wood|scrub" zoom-min="9" closed="yes">
			<!-- wood for SUMMER -->
			<rule cat="base_summer" e="way" k="*" v="*" >
				<rule e="way" k="*" v="*" zoom-max="13">
					<area fill="#80a0ca6c"/>
				</rule>
				<rule e="way" k="natural|landuse" v="forest|wood" zoom-min="14">
					<area fill="#80a0ca6c"/>
					<rule e="way" k="leaf_type" v="*" zoom-min="15">
						<rule e="way" k="leaf_type" v="needleleaved">
							<area src="file:patterns/wood_coniferous_transparent.svg" symbol-width="48"/>
						</rule>
						<rule e="way" k="leaf_type" v="broadleaved">
							<area src="file:patterns/wood_deciduous_transparent.svg" symbol-width="48"/>
						</rule>
						<rule e="way" k="leaf_type" v="mixed">
							<area src="file:patterns/wood_mixed_transparent.svg" symbol-width="48"/>
						</rule>
					</rule>
				</rule>
				<rule e="way" k="natural" v="scrub" zoom-min="13">
					<area fill="#30a0ca6c"/>
					<rule e="way" k="*" v="*" zoom-min="15">
						<area src="file:patterns/scrub.svg" symbol-width="20"/>
					</rule>
				</rule>
				<rule e="way" k="natural" v="fell" zoom-min="13">
					<area fill="#30a0ca6c"/>
				</rule>
			</rule>
			<!-- forest for WINTER -->
			<rule cat="base_winter" e="way" k="*" v="*" zoom-min="10">
				<rule e="way" k="natural|landuse" v="forest|wood" zoom-max="12">
					<area fill="#cce0e5"/>
				</rule>
				<rule e="way" k="natural|landuse" v="forest|wood" zoom-min="13">
					<area fill="$color_forest_winter_z13"/>
				</rule>
			</rule>
			<!-- forest for CAR -->
			<rule cat="base_car" e="way" k="*" v="*" zoom-min="13">
				<rule e="way" k="natural|landuse" v="forest|wood|scrub">
					<area fill="#80a0ca6c"/>
				</rule>
			</rule>
			<!-- wood for CITY -->
			<rule cat="base_city" e="way" k="*" v="*" zoom-min="13">
				<rule e="way" k="natural|landuse" v="forest|wood|scrub">
					<area fill="#80a0ca6c"/>
				</rule>
			</rule>
			<!-- wood captions only in topo styles -->
			<rule cat="base_summer" e="way" k="natural|landuse" v="forest|wood|scrub" zoom-min="14">
				<caption k="name" dy="12" font-style="bold" font-family="sans_serif" font-size="11" fill="#39761a"
						 stroke="#ffffff" stroke-width="2" text-wrap-width="$text_wrap_standard"/>
			</rule>
		</rule>

		<rule cat="base_summer" e="way" k="landuse" v="farm|farmlanf" closed="yes">
			<area fill="#f4f3e9"/>
		</rule>
		<!-- natural -->
		<rule cat="base_topo" e="way" k="natural" v="*">
			<rule e="way" k="natural" v="*" zoom-min="8" closed="yes">
				<rule e="way" k="natural" v="glacier">
					<area fill="#50D3E9FF"/>
					<area src="file:patterns/glacier.svg" symbol-width="20"/>
				</rule>
			</rule>
			<rule e="way" k="natural" v="*" zoom-min="11" closed="yes">
				<rule e="way" k="natural" v="beach">
					<rule e="way" k="tidal" v="no|~" closed="yes">
						<area fill="#FCF5E0"/>
					</rule>
					<area src="file:patterns/beach.png"/>
				</rule>
				<rule e="way" k="natural" v="heath">
					<area fill="#F1F2E9"/>
					<rule e="way" k="*" v="*" zoom-min="15">
						<area src="file:patterns/heath.svg" symbol-width="32"/>
					</rule>
				</rule>
				<rule e="way" k="natural" v="scree|shingle">
					<area fill="#80dddfe5"/>
					<area src="file:patterns/beach.png" symbol-width="96"/>
				</rule>
			</rule>
			<rule e="way" k="natural" v="*" zoom-min="13">
				<rule e="way" k="natural" v="bare_rock" closed="yes">
					<area src="file:patterns/bare_rock.png" symbol-width="96"/>
				</rule>
				<rule e="way" k="natural" v="cliff" zoom-min="14">
					<line stroke="#8a8a8a" stroke-width="0.2"/>
					<lineSymbol src="file:symbols/poi_cliff_way.svg" dy="0.1" repeat="true" repeat-gap="5" repeat-start="0" symbol-orientation="left" symbol-width="8" position="below" display="always"/>
				</rule>
				<rule e="way" k="natural" v="grassland" closed="yes" zoom-min="15">
					<area src="file:patterns/medow.svg" symbol-width="24"/>
				</rule>
				<rule e="way" k="natural" v="sand">
					<rule e="way" k="tidal" v="no|~" closed="yes">
						<area fill="#FCF5E0"/>
					</rule>
					<area src="file:patterns/beach.png"/>
				</rule>
			</rule>
			<!-- Natural captions -->
			<rule e="way" k="natural" v="heath|marsh|wetland|cliff" zoom-min="15">
				<caption k="name" font-style="normal" font-family="default" font-size="11" fill="#797979"
						 stroke="#ffffff" stroke-width="2" text-wrap-width="$text_wrap_standard"/>
			</rule>
			<rule e="way" k="natural" v="valley" zoom-min="14" zoom-max="17">
				<pathText k="name" dy="4.7" font-style="italic" font-family="default" font-size="17" fill="#797979" stroke="#ffffff" stroke-width="1.5"/>
			</rule>
		</rule>
		<!-- Power areals-->
		<rule cat="base_summer" e="way" k="power" v="generator|station|sub_station|substation" zoom-min="12" closed="yes">
			<area fill="#D6D6E1"/>
		</rule>
		<rule cat="base_winter" e="way" k="power" v="generator|station|sub_station|substation" zoom-min="12" closed="yes">
			<area fill="#CECECE"/>
		</rule>
		<!-- parks render after citi areas -->
		<rule e="way" k="leisure|landuse" v="park|village_green|recreation_ground" zoom-min="12" closed="yes">
			<!-- parks City -->
			<rule cat="base_city" e="way" k="*" v="*" zoom-min="13">
				<area fill="#80a0ca6c"/>
				<rule e="way" k="*" v="*" zoom-min="16">
					<caption k="name" dy="11"  font-family="sans_serif" font-size="12" fill="$color_attraction_poi_text"
							 stroke="#ffffff" stroke-width="3" position="below" priority="$priority_poi_caption_cat2"
							 text-wrap-width="$text_wrap_standard"/>
				</rule>
			</rule>
			<rule cat="base_summer" e="way" k="*" v="*">
				<area fill="#80a0ca6c"/>
				<rule e="way" k="*" v="*" zoom-min="16">
					<caption k="name" dy="11"  font-family="sans_serif" font-size="12" fill="${darken(opacity('#80a0ca6c',100), 40)}"
							 stroke="#ffffff" stroke-width="3" position="below" priority="$priority_poi_caption_cat2"
							 text-wrap-width="$text_wrap_standard"/>
				</rule>
			</rule>
			<!-- park for WINTER -->
			<rule cat="base_winter" e="way" k="*" v="*">
				<area fill="$color_park_orchard_winter"/>
				<rule e="way" k="*" v="*" zoom-min="16">
					<caption k="name" dy="11"  font-family="sans_serif" font-size="12" fill="${darken(opacity($color_park_orchard_winter,100), 50)}"
							 stroke="#ffffff" stroke-width="3" position="below" priority="$priority_poi_caption_cat2"
							 text-wrap-width="$text_wrap_standard"/>
				</rule>
			</rule>
			<rule cat="base_car" e="way" k="leisure" v="park" zoom-min="13">
				<area fill="#80a0ca6c"/>
				<rule e="way" k="*" v="*" zoom-min="16">
					<caption k="name" dy="12" font-style="bold" font-family="sans_serif" font-size="17" fill="#39761a"
							 stroke="#ffffff" stroke-width="3" position="below" priority="$priority_poi_caption_cat2" text-wrap-width="$text_wrap_standard"/>
				</rule>
			</rule>
		</rule>
		<!-- Orchards and meadows, cemetery -->
		<rule cat="base_topo" e="way" k="landuse" v="*" zoom-min="12">
			<rule e="way" k="*" v="*" zoom-min="13">
				<!-- meadow  -->
				<rule e="way" k="landuse" v="meadow|grass">
					<rule e="way" k="*" v="*" zoom-min="15">
						<area src="file:patterns/medow.svg" symbol-width="24"/>
					</rule>
					<rule e="way" k="*" v="*" zoom-min="16">
						<caption k="name" font-style="bold_italic" font-size="17" fill="#34972D" stroke="#FFFFFF"
								 stroke-width="1" text-wrap-width="$text_wrap_standard"/>
					</rule>
				</rule>
				<rule cat="base_summer" e="way" k="landuse" v="cemetery">
					<area fill="#80bdd16c" stroke="#e4e4e4" stroke-width="0.2"/>
					<rule e="way" k="*" v="*" zoom-min="13" zoom-max="15">
						<area src="file:patterns/graveyard.svg" symbol-width="24"/>
					</rule>
					<rule e="way" k="*" v="*" zoom-min="16">
						<area src="file:patterns/graveyard.svg" symbol-width="32"/>
					</rule>
					<rule e="way" k="*" v="grave_yard" zoom-min="16">
						<caption k="name" dy="11"  font-style="bold" font-family="sans_serif" font-size="17" fill="#39761a" stroke="#ffffff" stroke-width="2" text-wrap-width="$text_wrap_standard"/>
					</rule>

				</rule>
				<rule cat="base_winter" e="way" k="landuse" v="cemetery">
					<area fill="$color_forest_winter_z13" stroke="${darken($color_forest_winter_z13,30)}" stroke-width="0.2"/>
					<rule e="way" k="*" v="*" zoom-min="13" zoom-max="15">
						<area src="file:patterns/graveyard.svg" symbol-width="24"/>
					</rule>
					<rule e="way" k="*" v="*" zoom-min="16">
						<area src="file:patterns/graveyard.svg" symbol-width="32"/>
					</rule>
					<rule e="way" k="*" v="grave_yard" zoom-min="16">
						<caption k="name" dy="11"  font-style="bold" font-family="sans_serif" font-size="17" fill="#39761a" stroke="#ffffff" stroke-width="2" text-wrap-width="$text_wrap_standard"/>
					</rule>
				</rule>
			</rule>
			<rule e="way" k="*" v="*" zoom-min="12">
				<!-- vineyards, orchard -->
				<rule e="way" k="landuse" v="vineyard|orchard|hop_garden" closed="yes">
					<rule cat="base_summer" e="way" k="*" v="*">
						<area fill="#80bdd16c"/>
					</rule>
					<rule cat="base_winter" e="way" k="*" v="*">
						<area fill="$color_park_orchard_winter"/>
					</rule>
					<rule e="way" k="*" v="*" zoom-min="14" zoom-max="15">
						<rule e="way" k="landuse" v="vineyard">
							<area src="file:patterns/vineyard.svg" />
						</rule>
						<rule e="way" k="landuse" v="orchard">
							<area src="file:patterns/orchard.svg" />
						</rule>
						<rule e="way" k="landuse" v="hop_garden">
							<area src="file:patterns/hop_garden.svg" />
						</rule>
					</rule>
					<rule e="way" k="*" v="*" zoom-min="16">
						<rule e="way" k="landuse" v="vineyard">
							<area src="file:patterns/vineyard.svg" symbol-width="32"/>
						</rule>
						<rule e="way" k="landuse" v="orchard">
							<area src="file:patterns/orchard.svg" symbol-width="32"/>
						</rule>
						<rule e="way" k="landuse" v="hop_garden">
							<area src="file:patterns/hop_garden.svg" symbol-width="32"/>
						</rule>
					</rule>
				</rule>
			</rule>
		</rule>
		<!-- cemetery city -->
		<rule cat="base_city" e="way" k="landuse" v="*" zoom-min="13" closed="yes">
			<rule e="way" k="landuse" v="cemetery">
				<area fill="#80bdd16c" stroke="#e4e4e4" stroke-width="0.2"/>
				<rule e="way" k="*" v="*" zoom-min="13" zoom-max="15">
					<area src="file:patterns/graveyard.svg" symbol-width="24"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="16">
					<area src="file:patterns/graveyard.svg" symbol-width="32"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="16">
					<caption k="name" dy="11"  font-style="bold" font-family="sans_serif" font-size="17" fill="#39761a" stroke="#ffffff" stroke-width="2" text-wrap-width="$text_wrap_standard"/>
				</rule>
			</rule>
		</rule>
		<!-- amenity areas -->
		<rule cat="no_car" e="way" k="amenity" v="*" zoom-min="15" closed="yes">
			<rule cat="base_summer" e="way" k="*" v="grave_yard">
				<area fill="#80bdd16c" stroke="#e4e4e4" stroke-width="0.2"/>
				<rule e="way" k="*" v="*" zoom-min="13" zoom-max="15">
					<area src="file:patterns/graveyard.svg" symbol-width="24"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="16">
					<area src="file:patterns/graveyard.svg" symbol-width="32"/>
				</rule>
				<rule e="way" k="*" v="grave_yard" zoom-min="16">
					<caption k="name" dy="11"  font-style="bold" font-family="sans_serif" font-size="17" fill="#39761a" stroke="#ffffff" stroke-width="2" text-wrap-width="$text_wrap_standard"/>
				</rule>
			</rule>
			<rule cat="base_winter" e="way" k="amenity" v="grave_yard">
				<area fill="#8096bfbe" stroke="#e4e4e4" stroke-width="0.2"/>
				<rule e="way" k="*" v="*" zoom-min="13" zoom-max="15">
					<area src="file:patterns/graveyard.svg" symbol-width="24"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="16">
					<area src="file:patterns/graveyard.svg" symbol-width="32"/>
				</rule>
