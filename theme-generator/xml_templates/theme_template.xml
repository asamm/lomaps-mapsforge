						<rule e="way" k="*" v="*" zoom-min="18" >
							<line stroke="${opacity('#fff41d', 50)}" stroke-width="0.5" dy="${ -0.5 * ($piste_downhill_width + 0.5/2)}" />
							<lineSymbol src="file:symbols/poi_lightbulb.svg" dy="$( -0.5 * $piste_downhill_width)"
									symbol-orientation="left" symbol-width="14" repeat="true" repeat-gap="175"
										position="below" repeat-start="30" display="ifspace" scale="all"/>
						</rule>
					</rule>

					<rule e="way" k="*" v="*" closed="yes">
						<rule e="way" k="*" v="*" zoom-min="15" zoom-max="17">
							<lineSymbol src="file:symbols/poi_lightbulb.svg"
									symbol-orientation="left" symbol-width="14" repeat="true" repeat-gap="150"
										position="below" repeat-start="0" display="ifspace" scale="all"/>
						</rule>
						<rule e="way" k="*" v="*" zoom-min="18" >
							<lineSymbol src="file:symbols/poi_lightbulb.svg"
									symbol-orientation="left" symbol-width="14" repeat="true" repeat-gap="200"
										position="below" repeat-start="0" display="ifspace" scale="all"/>
						</rule>
					</rule>
				</rule>
			</rule>


			<!-- Nordic and Backcountry SKI -->
			<rule cat="pistes_nordic" e="way" k="piste:type" v="nordic|alpine|nordic;skitour">
				<rule e="way" k="*" v="*" zoom-min="12" zoom-max="15">
					<rule e="way" k="piste:difficulty" v="novice|easy|intermediate|~">
						<rule e="way" k="piste:grooming" v="classic|classic+skating|classic;skating|skating">
							<line stroke="#FFFFFF" stroke-width="${1.5 *  $pistes_nordic_width_z13}"/>
							<line stroke="$color_nordic_easy" stroke-width="$pistes_nordic_width_z13" />
						</rule>
						<rule e="way" k="piste:grooming" v="backcountry">
							<line stroke="#90FFFFFF" stroke-width="${1.5 * 0.6 * $pistes_nordic_width_z13}"/>
							<line stroke="$color_nordic_easy" stroke-width="${0.6 * $pistes_nordic_width_z13}" stroke-dasharray="4,3" stroke-linecap="round" scale="all"/>
						</rule>
						<rule e="way" k="piste:grooming" v="~">
							<line stroke="$color_nordic_easy" stroke-width="${0.6 * $pistes_nordic_width_z13}"/>
						</rule>
					</rule>
					<rule e="way" k="piste:difficulty" v="advanced|expert">
						<rule e="way" k="piste:grooming" v="classic|classic+skating|classic;skating|skating">
							<line stroke="#FFFFFF" stroke-width="${1.5 *  $pistes_nordic_width_z13}"/>
							<line stroke="$color_nordic_advanced" stroke-width="$pistes_nordic_width_z13"/>
						</rule>
						<rule e="way" k="piste:grooming" v="backcountry">
							<line stroke="#90FFFFFF" stroke-width="${1.5 * 0.6 * $pistes_nordic_width_z13}"/>
							<line stroke="$color_nordic_advanced" stroke-width="${0.6 * $pistes_nordic_width_z13}" stroke-dasharray="5,4" stroke-linecap="round" scale="all"/>
						</rule>
						<rule e="way" k="piste:grooming" v="~">
							<line stroke="$color_nordic_advanced" stroke-width="${0.6 * $pistes_nordic_width_z13}" stroke-dasharray="5,4" stroke-linecap="round" scale="all"/>
						</rule>
					</rule>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="16">
					<rule e="way" k="piste:difficulty" v="novice|easy|intermediate|~">
						<rule e="way" k="piste:grooming" v="classic|classic+skating|classic;skating|skating">
							<line stroke="#90FFFFFF" stroke-width="${1.5 *  $pistes_nordic_width_z16}"/>
							<line stroke="$color_nordic_easy" stroke-width="$pistes_nordic_width_z16"/>
						</rule>
						<rule e="way" k="piste:grooming" v="backcountry">
							<line stroke="#90FFFFFF" stroke-width="${1.5 * 0.6 * $pistes_nordic_width_z16}"/>
							<line stroke="$color_nordic_easy" stroke-width="${0.6 * $pistes_nordic_width_z16}" stroke-dasharray="2.5,2" stroke-linecap="round" scale="all" />
						</rule>
						<rule e="way" k="piste:grooming" v="~">
							<line stroke="$color_nordic_easy" stroke-width="${0.6 * $pistes_nordic_width_z16}"/>
						</rule>
					</rule>
					<rule e="way" k="piste:difficulty" v="advanced|expert">
						<rule e="way" k="piste:grooming" v="classic|classic+skating|classic;skating|skating">
							<line stroke="#90FFFFFF" stroke-width="${1.5 *  $pistes_nordic_width_z16}"/>
							<line stroke="$color_nordic_advanced" stroke-width="$pistes_nordic_width_z16"/>
						</rule>
						<rule e="way" k="piste:grooming" v="backcountry">
							<line stroke="#90FFFFFF" stroke-width="${1.5 * 0.6 * $pistes_nordic_width_z16}"/>
							<line stroke="$color_nordic_advanced" stroke-width="${0.6 * $pistes_nordic_width_z16}" stroke-dasharray="9,5" stroke-linecap="butt"/>
						</rule>
						<rule e="way" k="piste:grooming" v="~">
							<line stroke="$color_nordic_advanced" stroke-width="${0.6 * $pistes_nordic_width_z16}"/>
						</rule>
					</rule>
				</rule>

				<!-- Captions for nordic-->
				<rule e="way" k="*" v="*" zoom-min="14">
					<rule e="way" k="piste:difficulty" v="novice|easy|intermediate|~">
						<pathText k="name" font-style="bold" font-size="11" dy="9" fill="${darken($color_nordic_easy, 10)}" stroke="#ffffff"
								  stroke-width="2.5" repeat-gap="300" priority="$priority_tourist_route_name" scale="none" />
					</rule>
					<rule e="way" k="piste:difficulty" v="advanced|expert">
						<pathText k="name" font-style="bold" font-size="11" dy="9" fill="${darken($color_nordic_advanced, 10)}" stroke="#ffffff"
								  stroke-width="2.5" repeat-gap="300" priority="$priority_tourist_route_name"/>
					</rule>
				</rule>

				<!-- lit for nordic ski-->
				<rule e="way" k="lit|piste:lit" v="yes" zoom-min="16">
					<rule e="way" k="*" v="*" zoom-min="16" zoom-max="17">
						<lineSymbol src="file:symbols/poi_lightbulb.svg" dy="-$pistes_nordic_width_z16"
									symbol-orientation="left" symbol-width="14" repeat="true" repeat-gap="150"
									position="below" repeat-start="0" display="ifspace" scale="all"/>
					</rule>
					<rule e="way" k="*" v="*" zoom-min="18">
						<lineSymbol src="file:symbols/poi_lightbulb.svg" dy="-$pistes_nordic_width_z16"
									symbol-orientation="left" symbol-width="14" repeat="true" repeat-gap="250"
									position="below" repeat-start="0" display="ifspace" scale="all"/>
					</rule>
				</rule>

				<!--OneWay for piste nordic-->
				<rule e="way" k="piste:oneway" v="yes" zoom-min="15">
					<rule e="way" k="*" v="*" zoom-min="15" zoom-max="17">
						<lineSymbol src="file:symbols/poi_one_way_ski_nordic.svg" dy="${-1.5 * $pistes_nordic_width_z16}"
									symbol-orientation="left" symbol-width="14" repeat="true" repeat-gap="300"
									position="center" display="ifspace" priority="$priority_one_way"/>
					</rule>
					<rule e="way" k="*" v="*" zoom-min="18">
						<lineSymbol src="file:symbols/poi_one_way_ski_nordic.svg" dy="${-1.5 * $pistes_nordic_width_z16 }"
									symbol-orientation="left" symbol-width="18" repeat="true" repeat-gap="400"
									position="center" display="ifspace" priority="$priority_one_way"/>
					</rule>
				</rule>
			</rule>

			<!-- Ski Hike -->
			<rule e="way" k="piste:type" v="hike" zoom-min="13">
				<rule e="way" k="*" v="*" zoom-min="13">
					<line stroke="#90FFFFFF" stroke-width="1.7"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="13" zoom-max="14">
					<line stroke="$color_piste_hike" stroke-width="${1.3 * $pistes_nordic_width_z13}" stroke-dasharray="0.1,5" scale="all"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="15" zoom-max="16">
					<line stroke="$color_piste_hike" stroke-width="${1 * $pistes_nordic_width_z13}" stroke-dasharray="0.1,3" scale="all"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="17" >
					<line stroke="$color_piste_hike" stroke-width="${1 * $pistes_nordic_width_z16}" stroke-dasharray="0.1,2" scale="all"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="15">
					<pathText k="name" font-style="bold" font-size="11" dy="9" fill="${darken($color_piste_hike, 10)}" stroke="#ffffff"
								  stroke-width="2.5" repeat-gap="300" priority="$priority_tourist_route_name" scale="none" />
				</rule>
			</rule>

			<!-- Ski Tours -->
			<rule e="way" k="piste:type" v="skitour">
				<rule e="way" k="*" v="*" zoom-min="13" zoom-max="14">
					<line stroke="#CCFFFFFF" stroke-width="${1.6 * 0.9 * $pistes_nordic_width_z13}"/>
					<line stroke="$color_piste_skitour" stroke-width="${ 0.9 *$pistes_nordic_width_z13}" stroke-dasharray="5,4" scale="all"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="15" zoom-max="16">
					<line stroke="#CCFFFFFF" stroke-width="${1.6 * 0.7 * $pistes_nordic_width_z13}"/>
					<line stroke="$color_piste_skitour" stroke-width="${ 0.7 * $pistes_nordic_width_z13}" stroke-dasharray="4,3.2" scale="all"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="17" >
					<line stroke="#CCFFFFFF" stroke-width="${1.6 * 0.8 * $pistes_nordic_width_z16}"/>
					<line stroke="$color_piste_skitour" stroke-width="${ 0.8 * $pistes_nordic_width_z16}" stroke-dasharray="2,2" scale="all"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="15">
					<pathText k="name" font-style="bold" font-size="11" dy="10" fill="${darken($color_piste_skitour, 20)}" stroke="#ffffff"
								  stroke-width="2.5" repeat-gap="300" priority="$priority_tourist_route_name" scale="none" />
				</rule>
			</rule>

			<rule e="way" k="piste:type" v="sled" zoom-min="14">
				<rule e="way" k="*" v="*" zoom-min="14" zoom-max="15">
					<line stroke="#CCFFFFFF" stroke-width="${1.6 * 0.6 * $pistes_nordic_width_z13}"/>
					<line stroke="$color_piste_sled" stroke-width="${0.6 * $pistes_nordic_width_z13}"/>
				</rule>
				<rule e="way" k="*" v="*" zoom-min="16" >
					<line stroke="#CCFFFFFF" stroke-width="${1.6 * 0.6 * $pistes_nordic_width_z16}"/>
					<line stroke="$color_piste_sled" stroke-width="${0.6 * $pistes_nordic_width_z16}"/>
				</rule>
			</rule>

		</rule>
		<!-- end of pistes -->
		<!-- aerial ways only for topo themes-->
		<rule cat="base_topo" e="way" k="aerialway" v="*" zoom-min="12">
			<rule e="way" k="*" v="*" zoom-min="12" zoom-max="13">
				<rule e="way" k="aerialway" v="chair_lift|mixed_lift">
					<line stroke="#3a3a3a" stroke-width="0.6" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="2" stroke-dasharray="10,14" stroke-linecap="butt"/>
				</rule>
				<rule e="way" k="aerialway" v="cable_car|gondola">
					<line stroke="$color_aerialway_core" stroke-width="0.8" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="3" stroke-dasharray="1,20"/>
					<line stroke="$color_aerialway_cablecar_fill" stroke-width="1.8" stroke-dasharray="1,20"/>
				</rule>
				<rule e="way" k="aerialway" v="goods">
					<line stroke="$color_aerialway_core" stroke-width="0.5" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="1.5" stroke-dasharray="4,20"/>
				</rule>
				<rule e="way" k="aerialway" v="drag_lift">
					<line stroke="$color_aerialway_core" stroke-width="0.8" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="1.8" stroke-dasharray="0,6,1,6"/>
				</rule>
			</rule>
			<rule e="way" k="*" v="*" zoom-min="14" zoom-max="15">
				<rule e="way" k="aerialway" v="chair_lift|mixed_lift">
					<line stroke="$color_aerialway_core" stroke-width="0.50" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="1.4" stroke-dasharray="16,22" stroke-linecap="butt"/>
				</rule>
				<rule e="way" k="aerialway" v="cable_car|gondola">
					<line stroke="$color_aerialway_core" stroke-width="0.6" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="2.4" stroke-dasharray="1,26"/>
					<line stroke="$color_aerialway_cablecar_fill" stroke-width="1.5" stroke-dasharray="1,26"/>
				</rule>
				<rule e="way" k="aerialway" v="goods">
					<line stroke="$color_aerialway_core" stroke-width="0.5" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="1.5" stroke-dasharray="8,28"/>
				</rule>
				<rule e="way" k="aerialway" v="drag_lift|t-bar|j-bar|platter">
					<line stroke="$color_aerialway_core" stroke-width="0.4" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="1.1" stroke-dasharray="0,8,1,8"/>
				</rule>
			</rule>
			<rule e="way" k="*" v="*" zoom-min="16">
				<rule e="way" k="aerialway" v="chair_lift|mixed_lift">
					<line stroke="$color_aerialway_core" stroke-width="0.25" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="0.8" stroke-dasharray="25,30" stroke-linecap="butt"/>
				</rule>
				<rule e="way" k="aerialway" v="cable_car|gondola">
					<line stroke="$color_aerialway_core" stroke-width="0.27" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="1.3" stroke-dasharray="1,32"/>
					<line stroke="$color_aerialway_cablecar_fill" stroke-width="0.8" stroke-dasharray="1,32"/>
				</rule>
				<rule e="way" k="aerialway" v="goods">
					<line stroke="$color_aerialway_core" stroke-width="0.3" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="1" stroke-dasharray="11,36"/>
				</rule>
				<rule e="way" k="aerialway" v="drag_lift|t-bar|j-bar|platter|rope_tow">
					<line stroke="$color_aerialway_core" stroke-width="0.26" stroke-linecap="butt"/>
					<line stroke="$color_aerialway_core" stroke-width="0.7" stroke-dasharray="0,11,1,11"/>
				</rule>
			</rule>
			<!-- print names of aerial wasy only for SKI -->
			<rule cat="base_winter" e="way" k="*" v="*" zoom-min="16">
				<pathText k="name" font-style="italic" font-family="serif" font-size="11" dy="-1.5"
						  fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="2.5" repeat-gap="300"/>
			</rule>
		</rule>
		<!-- OSMC SYMBOLS and hiking routes without color -->
		<rule cat="hik_routes" e="way" k="route" v="hiking|foot">
			<!-- HIKING PATH OSMC:SYMBOL-->
			<rule e="way" k="osmc" v="yes" zoom-min="15" zoom-max="19">
				<!--display only elements with defined foreground to avoid empty backgroud symbols-->
				<rule e="way" k="osmc_foreground" v="*">
					<rule e="way" k="osmc_order" v="~" gen_section="osmc_symbols">
						<rule e="way" k="osmc_foreground" v="black">
							<lineSymbol src="file:osmc/frg_black_arch.svg" dy="-18" scale="none" symbol-width="12"
										symbol-orientation="left" display="always" position="below_right"
										repeat-gap="400" repeat="true" priority="$priority_tourist_osmc_symbol"
										repeat-start="$osmc_symbol_repeat_start"/>

						</rule>
					</rule>
					<generator action="$gen_action_osmc_symbols_and_order" source_section="osmc_symbols"/>
				</rule>
			</rule
		</rule>
		<!-- END OF OSMC_SYMBOLS FOR HIKING -->>
		<!-- Power lines -->
		<rule cat="base_topo" e="way" k="power" v="*" zoom-min="13">
			<rule e="way" k="power" v="line" zoom-min="13">
				<line stroke="#707070" stroke-width="0.2" stroke-dasharray="10,3,1,3"/>
			</rule>
		</rule>
	</rule>
	<!-- END OF WAYS-->

	<!-- ===================================================
						   POIS
	==================================================== -->
	<rule cat="base_car" e="any" k="highway" v="motorway_junction">
		<rule e="any" k="*" v="*" zoom-min="12" zoom-max="13">
			<circle r="2.5" fill="#ffffff" stroke="#ff5b58" stroke-width="0.5 "/>
		</rule>
		<rule e="any" k="*" v="*" zoom-min="14" zoom-max="14">
			<caption k="name" dy="-10" font-style="bold" font-size="13" fill="#d97f05" stroke="#ffffff" stroke-width="2.5" text-wrap-width="$text_wrap_standard"/>
		</rule>
	</rule>
	<!-- Tourism attraction before any other pois because lower priority-->
	<rule cat="tourism_city" e="any" k="tourism" v="*" zoom-min="15">
		<rule e="any" k="tourism" v="attraction">
			<caption k="name" dy="11"  font-style="bold" font-family="sans_serif" font-size="14" fill="#826f60" stroke="#ffffff" stroke-width="1.5" text-wrap-width="$text_wrap_standard"/>
		</rule>
	</rule>
	<rule cat="tourism_topo" e="any" k="tourism" v="*" zoom-min="15">
		<rule e="any" k="tourism" v="attraction">
			<caption k="name" dy="11"  font-style="bold" font-family="sans_serif" font-size="14" fill="#826f60" stroke="#ffffff" stroke-width="1.5" text-wrap-width="$text_wrap_standard"/>
		</rule>
	</rule>
	<!-- amenity -->
	<rule e="any" k="amenity" v="*" zoom-min="13">
		<rule cat="base_topo" e="any" k="amenity" v="hospital" zoom-min="16">
			<caption k="name" dy="20" font-style="italic" font-family="serif" font-size="12" fill="$color_standard_poi_text"
					 stroke="#ffffff" stroke-width="3" priority="$priority_poi_caption_cat1" text-wrap-width="$text_wrap_standard"/>
		</rule>
		<rule e="any" k="amenity" v="*" zoom-min="14">
			<!-- church or cathedral for topo -->
			<rule cat="base_topo" e="any" k="amenity" v="place_of_worship" zoom-min="16">
				<!-- captions > 16 -->
				<rule e="any" k="*" v="*" zoom-min="16">
					<caption k="name" dy="11"  font-family="sans_serif" font-size="12" fill="$color_attraction_poi_text"
							 stroke="#ffffff" stroke-width="3" position="below" priority="$priority_poi_caption_cat1" text-wrap-width="$text_wrap_standard"/>
				</rule>
			</rule>
		</rule>

		<rule e="any" k="amenity" v="*" zoom-min="15">
			<rule cat="city" e="any" k="amenity" v="*">
				<rule e="any" k="*" v="hospital" >
					<caption k="name" dy="11"  font-style="bold_italic" font-family="serif" font-size="11" fill="#333333" stroke="#ffffff" stroke-width="2.5" text-wrap-width="$text_wrap_standard"/>
				</rule>
				<rule cat="religion_city" e="any" k="amenity" v="place_of_worship" zoom-min="17">
					<caption k="name" dy="11"  font-style="bold_italic" font-family="sans_serif" font-size="13" fill="#826f60" stroke="#ffffff" stroke-width="1.5" text-wrap-width="$text_wrap_standard"/>
				</rule>
				<rule e="any" k="amenity" v="university|theatre" zoom-min="16">
					<caption k="name" dy="11"  font-style="bold" font-family="sans_serif" font-size="13" fill="#826f60" stroke="#ffffff" stroke-width="1.5" text-wrap-width="$text_wrap_standard"/>
				</rule>
				<rule cat="pub_rest_city" e="any" k="*" v="*">
					<!-- Captions for city pubs > 18 -->
					<rule e="any" k="amenity" v="pub|bar|restaurant|cafe|fast_food" zoom-min="18">
						<caption k="name" dy="11"  font-style="bold" font-family="sans_serif" font-size="13" fill="#826f60" stroke="#ffffff" text-wrap-width="$text_wrap_standard"/>
					</rule>
				</rule>
			</rule>
			<!--  CAR amenities > 17 -->
			<rule cat="base_car" e="any" k="amenity" v="*" zoom-min="16">
				<rule e="any" k="amenity" v="pub|bar|restaurant|cafe|fast_food" zoom-min="18">
					<caption k="name" dy="11"  font-style="bold" font-family="sans_serif" font-size="13" fill="#826f60" stroke="#ffffff" text-wrap-width="$text_wrap_standard"/>
				</rule>
				<rule e="any" k="amenity" v="college|university|school|theatre|hospital">
					<caption k="name" dy="11"  font-style="bold" font-family="serif" font-size="11" fill="#333333" stroke="#ffffff" stroke-width="2" text-wrap-width="$text_wrap_standard"/>
				</rule>
			</rule>
		</rule>
		<rule e="any" k="amenity" v="*" zoom-min="17">
			<rule e="any" k="amenity" v="bus_station">
				<caption k="name" dy="20" font-style="bold" font-size="11" fill="#333333" stroke="#ffffff" stroke-width="2" text-wrap-width="$text_wrap_standard"/>
			</rule>
			<!-- labels from zl > 17-->
			<rule e="any" k="amenity" v="college|university|school|theatre|cinema|library">
				<caption k="name" dy="12" font-family="sans_serif" font-size="12" fill="$color_gray_poi_text" position="below" stroke="#ffffff" stroke-width="3" priority="$priority_poi_caption_cat1" text-wrap-width="$text_wrap_standard"/>
			</rule>
		</rule>
	</rule><!-- end of amenity -->

	<!-- barrier -->
	<rule cat="base_topo" e="any" k="barrier" v="*" zoom-min="15">
		<rule e="any" k="barrier" v="bollard">
			<circle r="1.5" fill="#707070"/>
		</rule>
        <!-- In zoom level 15 set smaller size for gates -->
        <rule e="any" k="*" v="*" zoom-min="15" zoom-max="16">
            <rule e="any" k="barrier" v="gate|lift_gate|swing_gate" >
