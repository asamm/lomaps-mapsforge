                    <caption k="name" text-transform="uppercase" font-family="sans_serif" font-style="bold"
							 font-size="30" fill="$color_standard_poi_text" stroke="#FFFFFF"
							 stroke-width="${$text_halo*1.2}" priority="$priority_city_1" text-wrap-width="${round(1.5 * $text_wrap_standard)}"/>
                </rule>
                <rule e="any" k="capital" v="~" >
                    <caption k="name" text-transform="uppercase" font-family="sans_serif" font-style="bold"
							 font-size="24" fill="$color_standard_poi_text" stroke="#FFFFFF"
							 stroke-width="${$text_halo*1.2}" priority="$priority_city_1" text-wrap-width="${round(1.5 * $text_wrap_standard)}"/>
                </rule>
			</rule>
			<rule e="any" k="*" v="*" zoom-min="6" zoom-max="9">
				<circle r="3.5"  stroke="#8E8E8E" stroke-width="1.5" />
			</rule>
		</rule>

		<rule e="any" k="place" v="town" zoom-min="8" zoom-max="15">
			<rule e="any" k="*" v="*" zoom-min="8" zoom-max="9">
				<caption k="name" dy="11" font-family="sans_serif"  font-size="12" fill="$color_standard_poi_text"
						 stroke="#ffffff" stroke-width="${$text_halo}" priority="$priority_town_1" text-wrap-width="$text_wrap_standard"/>
            </rule>
			<rule e="any" k="*" v="*" zoom-min="10" zoom-max="11">
				<caption k="name" dy="11"  font-family="sans_serif"  font-size="16" fill="$color_standard_poi_text"
						 stroke="#ffffff" stroke-width="${$text_halo*1.1}" priority="$priority_town_1" text-wrap-width="$text_wrap_standard"/>
			</rule>
			<rule e="any" k="*" v="*" zoom-min="12" zoom-max="14">
				<caption k="name" font-family="sans_serif" font-style="bold" font-size="22" fill="$color_standard_poi_text"
						 stroke="#ffffff" stroke-width="${$text_halo*1.2}" priority="$priority_town_1" text-wrap-width="${round(1.5 * $text_wrap_standard)}"/>
			</rule>
            <rule e="any" k="*" v="*" zoom-min="15" zoom-max="15">
				<caption k="name" text-transform="uppercase" font-family="sans_serif" font-style="bold" font-size="26" fill="$color_standard_poi_text"
						 stroke="#ffffff" stroke-width="${$text_halo*1.2}" priority="$priority_town_1" text-wrap-width="${round(2 * $text_wrap_standard)}"/>
			</rule>


			<!-- circles for towns -->
			<rule e="any" k="place" v="town" zoom-min="9" zoom-max="9">
				<rule e="any" k="*" v="*" zoom-min="9" zoom-max="9">
					<circle r="3" fill="#f4f3e9" stroke="#9B9B9B" stroke-width="1"/>
					<circle r="1.1" stroke="#9B9B9B" stroke-width="1"/>
				</rule>
				<rule e="any" k="*" v="*" zoom-min="10" zoom-max="10">
					<circle r="4" fill="#f4f3e9" stroke="#9B9B9B" stroke-width="1"/>
					<circle r="1.6" stroke="#9B9B9B" stroke-width="1"/>
				</rule>
			</rule>
		</rule>

		<rule e="any" k="place" v="village" zoom-min="12" zoom-max="16">
			<rule e="any" k="*" v="*" zoom-min="12" zoom-max="13">
				<caption k="name" font-family="sans_serif" font-size="17" fill="$color_standard_poi_text" stroke="#ffffff"
						 stroke-width="${$text_halo}" priority="$priority_village_1" text-wrap-width="$text_wrap_standard"/>
			</rule>
			<rule e="any" k="*" v="*" zoom-min="14" zoom-max="16">
				<caption k="name" font-family="sans_serif" font-style="bold" font-size="20" fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="$text_halo" priority="$priority_village_1" text-wrap-width="$text_wrap_standard"/>
			</rule>
		</rule>

		<rule e="any" k="place" v="suburb" zoom-min="13" zoom-max="16">
			<rule e="any" k="*" v="*" zoom-min="13" zoom-max="14">
				<caption k="name" font-size="14" fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="${$text_halo}" priority="$priority_suburb" text-wrap-width="$text_wrap_standard"/>
			</rule>
			<rule e="any" k="*" v="*" zoom-min="15" zoom-max="16">
				<caption k="name" font-size="15" text-transform="uppercase" fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="$text_halo" priority="$priority_suburb" text-wrap-width="$text_wrap_standard"/>
			</rule>
		</rule>

		<rule e="any" k="place" v="hamlet" zoom-min="13" zoom-max="16">
			<rule e="any" k="*" v="*" zoom-min="13" zoom-max="14">
				<caption k="name" font-family="sans_serif" font-size="15" fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="$text_halo" priority="$priority_hamlet" text-wrap-width="$text_wrap_standard"/>
			</rule>
			<rule e="any" k="*" v="*" zoom-min="15" zoom-max="16">
				<caption k="name" font-family="sans_serif" font-style="bold" font-size="17" fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="$text_halo" priority="$priority_hamlet" text-wrap-width="$text_wrap_standard"/>
			</rule>
		</rule>

		<!-- TODO country="*|!ES" -->
		<rule e="any" k="place" v="locality" zoom-min="15">
			<caption k="name" font-size="12" fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="${$text_halo}" priority="$priority_locality" text-wrap-width="$text_wrap_standard"/>
		</rule>

		<rule e="any" k="place" v="island" zoom-min="8" zoom-max="14">
			<caption k="name" font-style="italic" font-size="17" fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="${$text_halo / 2}" text-wrap-width="$text_wrap_standard" priority="$priority_island"/>
		</rule>
		<rule e="any" k="place" v="islet" zoom-min="14" zoom-max="17">
			<caption k="name" font-style="italic" font-size="12" fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="${$text_halo / 2}" text-wrap-width="$text_wrap_standard" priority="$priority_islet"/>
		</rule>
		<rule e="any" k="place" v="country" zoom-max="6">
			<caption k="name" font-style="bold" text-transform="uppercase" font-size="28" fill="$color_standard_poi_text" stroke="#ffffff" stroke-width="2.0" text-wrap-width="$text_wrap_standard"/>
		</rule>
	</rule>
</rendertheme>